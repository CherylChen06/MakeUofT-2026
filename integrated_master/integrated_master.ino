// Timing variables
unsigned long previousMillis = 0;
const long interval = 500;
int frameCounter = 0;
void setup() {
Serial.begin(9600);
BT.begin(9600);
if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
Serial.println(F("SSD1306 allocation failed"));
for(;;);
}
pinMode(SPEAKER_PIN, OUTPUT);
pinMode(BUTTON_SELECT_PIN, INPUT_PULLUP);
pinMode(SWITCH_PIN, INPUT);
pinMode(BUTTON_ENTER_PIN, INPUT_PULLUP);
Serial.println("MASTER: Booted, waiting for connection");
display.clearDisplay();
currentState = STATE_IDLE;
}
void loop() {
// Check ENTERING button (Button 2)
if (digitalRead(BUTTON_ENTER_PIN) == LOW) {
delay(200); // Debounce
// Check for Bluetooth connection before proceeding
systemState = WAIT_FOR_CONNECTION;

while(systemState != PLAYING) {
  switch(systemState){
    case WAIT_FOR_CONNECTION:
      Serial.println("MASTER: Waiting for Bluetooth connection...");
      BT.write('P'); // Send ping
      
      if(BT.available()){
        char c = BT.read();
        Serial.print("MASTER: Received from slave -> ");
        Serial.println(c);
        systemState = SEND_SIGNAL;
      }
      delay(500);
      break;

    case SEND_SIGNAL:
      Serial.println("MASTER: Sending START signal to slave");
      BT.write('S');
      systemState = ANIMATING;
      break;

    case ANIMATING:
      Serial.println("MASTER: Running animation");
      runAnimation();
      systemState = PLAYING;
      break;

    case PLAYING:
      Serial.println("MASTER: Playing music");
      playMusic();
      // Stay in PLAYING state - will exit while loop
      break;
  }
}

// After music plays, freeze on last frame
while(true) {
  // Do nothing forever - screen frozen
}
}
// Check SELECT button (Button 1)
if (digitalRead(BUTTON_SELECT_PIN) == LOW) {
if (digitalRead(SWITCH_PIN) == HIGH) {
currentState = STATE_MALE;
} else {
currentState = STATE_FEMALE;
}
delay(200); // Debounce
}
// Handle idle animations
unsigned long currentMillis = millis();
if (currentMillis - previousMillis >= interval) {
previousMillis = currentMillis;
frameCounter = !frameCounter;
display.clearDisplay();

switch (currentState) {
  case STATE_IDLE:
    if(frameCounter == 0) display.drawBitmap(0, 0, idle1, 128, 64, WHITE);
    else                  display.drawBitmap(0, 0, idle2, 128, 64, WHITE);
    break;

  case STATE_MALE:
    if(frameCounter == 0) display.drawBitmap(0, 0, male1, 128, 64, WHITE);
    else                  display.drawBitmap(0, 0, male2, 128, 64, WHITE);
    break;

  case STATE_FEMALE:
    if(frameCounter == 0) display.drawBitmap(0, 0, female1, 128, 64, WHITE);
    else                  display.drawBitmap(0, 0, female2, 128, 64, WHITE);
    break;
}
display.display();
}
}
void runAnimation(){
Serial.println("MASTER animation running...");
int speed = 300;
// Play entering animation
display.clearDisplay();
display.drawBitmap(0, 0, maleEnter1, 128, 64, WHITE);
display.display();
delay(speed);
display.clearDisplay();
display.drawBitmap(0, 0, maleEnter2, 128, 64, WHITE);
display.display();
delay(speed);
display.clearDisplay();
display.drawBitmap(0, 0, maleEnter3, 128, 64, WHITE);
display.display();
delay(speed);
// Final frame stays on screen
display.clearDisplay();
display.drawBitmap(0, 0, maleEnter4, 128, 64, WHITE);
display.display();
}
void playMusic(){
Serial.println("MASTER music playing...");
for(int i=0; i<33; i++){
tone(SPEAKER_PIN, notes[i], dur[i]);
delay(dur[i]*2.8);
}
noTone(SPEAKER_PIN);
}