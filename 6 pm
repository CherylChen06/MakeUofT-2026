// --- TIMING VARIABLES ---
unsigned long previousMillis = 0;
const long interval = 500;   // Speed of animation (500ms)
int frameCounter = 0;        // Keeps track of which frame to show (0 or 1)

void setup() {
  Serial.begin(9600);
  
  // Initialize Display
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) { 
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }

  // Initialize Pins
  // INPUT_PULLUP means the pin is HIGH (5V) by default. 
  // When you press the button (connect to GND), it reads LOW.
  pinMode(BUTTON_PIN, INPUT_PULLUP); 
  pinMode(SWITCH_PIN, INPUT); 

  display.clearDisplay();
}

void loop() {
  // --- 1. READ INPUTS ---
  // Read the button (LOW means pressed because of pull-up)
  if (digitalRead(BUTTON_PIN) == LOW) {
    
    // Check the switch position
    if (digitalRead(SWITCH_PIN) == HIGH) {
      currentState = STATE_MALE;   // Left Side (5V)
    } else {
      currentState = STATE_FEMALE; // Right Side (GND)
    }
    
    // Tiny delay to prevent button bouncing
    delay(200); 
  }

  // --- 2. HANDLE ANIMATIONS ---
  unsigned long currentMillis = millis();

  if (currentMillis - previousMillis >= interval) {
    previousMillis = currentMillis;
    
    // Toggle frame counter between 0 and 1
    frameCounter = !frameCounter; 

    display.clearDisplay();

    // Decide which image to draw based on STATE and FRAME
    switch (currentState) {
      
      case STATE_IDLE:
        if(frameCounter == 0) display.drawBitmap(0, 0, idle1, 128, 64, WHITE);
        else                  display.drawBitmap(0, 0, idle2, 128, 64, WHITE);
        break;

      case STATE_MALE:
        if(frameCounter == 0) display.drawBitmap(0, 0, male1, 128, 64, WHITE);
        else                  display.drawBitmap(0, 0, male2, 128, 64, WHITE);
        break;

      case STATE_FEMALE:
        if(frameCounter == 0) display.drawBitmap(0, 0, female1, 128, 64, WHITE);
        else                  display.drawBitmap(0, 0, female2, 128, 64, WHITE);
        break;
    }

    display.display();
  }
}
