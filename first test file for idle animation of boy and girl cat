// --- TIMING VARIABLES ---
unsigned long previousMillis = 0;  // Stores the last time the image was updated
const long interval = 500;         // Interval at which to blink (milliseconds)
bool showFrame1 = true;            // State variable to track which frame is active

void setup() {
  Serial.begin(9600);
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) { 
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
}

void loop() {
  // 1. GET CURRENT TIME
  unsigned long currentMillis = millis();

  // 2. CHECK IF IT IS TIME TO SWITCH FRAMES
  if (currentMillis - previousMillis >= interval) {
    // Save the last time you switched the frame
    previousMillis = currentMillis;

    display.clearDisplay();

    // Toggle the state
    if (showFrame1) {
      display.drawBitmap(0, 0, epd_bitmap_BASE_CAT_ANI_1, 128, 64, WHITE);
      showFrame1 = false; // Next time, show frame 2
    } else {
      display.drawBitmap(0, 0, epd_bitmap_BASE_CAT_ANI_2, 128, 64, WHITE);
      showFrame1 = true;  // Next time, show frame 1
    }

    display.display();
  }

  // 3. RUN OTHER ROBOT CODE HERE (Non-blocking!)
  // Because we didn't use delay(), this code runs thousands of times per second
  // even while the screen is waiting to switch.
  
  // readSensors();
  // driveMotors();
  // checkButtons();
}
