// --- TIMING VARIABLES ---
unsigned long previousMillis = 0;
const long interval = 500;   // Speed of idle animation
int frameCounter = 0;        

void setup() {
  Serial.begin(9600);
  
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) { 
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }

  // --- PIN MODES ---
  pinMode(BUTTON_SELECT_PIN, INPUT_PULLUP); 
  pinMode(SWITCH_PIN,        INPUT); 
  pinMode(BUTTON_ACTION_PIN, INPUT_PULLUP); // New Button 2

  display.clearDisplay();
}

void loop() {
  
  // ==========================================
  // 1. CHECK ATTACK BUTTON (BUTTON 2)
  // ==========================================
  // If button 2 is pressed AND we are not in the generic IDLE state
  if (digitalRead(BUTTON_ACTION_PIN) == LOW && currentState != STATE_IDLE) {
    
    // Play the specific animation sequence based on current gender
    if (currentState == STATE_MALE) {
      playFinalAnimation(maleMove1, maleMove2, maleMove3);
    } else {
      playFinalAnimation(femaleMove1, femaleMove2, femaleMove3);
    }

    // Clear Screen and FREEZE
    display.clearDisplay();
    display.display();
    
    // Infinite loop: "Render useless" until reset
    while(true) {
      // Do nothing forever
    }
  }

  // ==========================================
  // 2. CHECK SELECT BUTTON (BUTTON 1)
  // ==========================================
  if (digitalRead(BUTTON_SELECT_PIN) == LOW) {
    if (digitalRead(SWITCH_PIN) == HIGH) {
      currentState = STATE_MALE;
    } else {
      currentState = STATE_FEMALE;
    }
    delay(200); // Debounce
  }

  // ==========================================
  // 3. HANDLE IDLE ANIMATIONS
  // ==========================================
  unsigned long currentMillis = millis();

  if (currentMillis - previousMillis >= interval) {
    previousMillis = currentMillis;
    frameCounter = !frameCounter; 

    display.clearDisplay();

    switch (currentState) {
      case STATE_IDLE:
        if(frameCounter == 0) display.drawBitmap(0, 0, idle1, 128, 64, WHITE);
        else                  display.drawBitmap(0, 0, idle2, 128, 64, WHITE);
        break;

      case STATE_MALE:
        if(frameCounter == 0) display.drawBitmap(0, 0, male1, 128, 64, WHITE);
        else                  display.drawBitmap(0, 0, male2, 128, 64, WHITE);
        break;

      case STATE_FEMALE:
        if(frameCounter == 0) display.drawBitmap(0, 0, female1, 128, 64, WHITE);
        else                  display.drawBitmap(0, 0, female2, 128, 64, WHITE);
        break;
    }
    display.display();
  }
}

// ==========================================
// HELPER: Play 4-frame animation then stop
// ==========================================
void playFinalAnimation(const unsigned char* m1, const unsigned char* m2, const unsigned char* m3) {
  int speed = 300; // Animation speed in milliseconds

  display.clearDisplay(); display.drawBitmap(0, 0, m1, 128, 64, WHITE); display.display();
  delay(speed);
  
  display.clearDisplay(); display.drawBitmap(0, 0, m2, 128, 64, WHITE); display.display();
  delay(speed);
  
  display.clearDisplay(); display.drawBitmap(0, 0, m3, 128, 64, WHITE); display.display();
  delay(speed);

}
