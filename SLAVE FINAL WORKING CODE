//starting screen to select from the two available
//slide to chosen one and then click to decide gender
//clicking the second button is



#include <Wire.h>
#include <Adafruit_GFX.h>
#include <Adafruit_SSD1306.h>

#define SCREEN_WIDTH 128
#define SCREEN_HEIGHT 64
#define OLED_RESET     -1 
Adafruit_SSD1306 display(SCREEN_WIDTH, SCREEN_HEIGHT, &Wire, OLED_RESET);

// --- PIN DEFINITIONS ---
const int BUTTON_SELECT_PIN = 2;  // Button 1: Switch Gender (Pin 2 + GND)
const int SWITCH_PIN        = 3;  // Toggle Switch: Male/Female (Pin 3 + GND/5V)
const int BUTTON_ACTION_PIN = 4;  // Button 2: ATTACK & END (Pin 4 + GND)

// --- STATES ---
enum State {
  STATE_IDLE,
  STATE_MALE,
  STATE_FEMALE
};
State currentState = STATE_IDLE; // Start in IDLE mode

// ==========================================
//               IMAGE DATA
// ==========================================

